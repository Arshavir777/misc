{
  "name": "quiz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "prettier:base": "prettier --ignore-path .gitignore",
    "eslint:base": "eslint --ignore-path .gitignore",
    "lint:eslint": "npm run eslint:base -- --max-warnings 0 \"**/*.{js,vue}\"",
    "lint:eslint-config-prettier": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "lint": "npm run lint:eslint",
    "format:prettier": "npm run prettier:base -- --write \"**/*.{js,ts,tsx,json,md,mdx}\"",
    "format:eslint": "npm run eslint:base -- --fix \"**/*.{js,ts,tsx,mdx}\"",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "vue-cli-service test:e2e --mode qa",
    "build-prod": "vue-cli-service build --mode production",
    "build-qa": "vue-cli-service build --mode qa",
    "deploy-prod": "node scripts/deploy-prod.js",
    "deploy-qa": "node scripts/deploy-qa.js",
    "push-aws-prod": "deploy-aws-s3-cloudfront --bucket app.getlasting.com --source ./dist --delete true --debug true --non-interactive true",
    "push-aws-qa": "deploy-aws-s3-cloudfront --bucket app-qa.getlasting.com --source ./dist --delete true --debug true --non-interactive true"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "dayjs": "~1.10.2",
    "file-saver": "^2.0.5",
    "js-cookie": "~2.2.1",
    "mixpanel": "~0.13.0",
    "mixpanel-browser": "2.41.0",
    "sass": "^1.34.0",
    "uuid": "~8.3.2",
    "vue": "~2.6.13",
    "vue-router": "~3.5.1",
    "vue-stripe-elements-plus": "~0.3.2",
    "vuelidate": "~0.7.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@cypress/vue": "^2.2.3",
    "@cypress/webpack-dev-server": "^1.3.1",
    "@vue/cli": "~4.5.13",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-service": "~4.5.13",
    "babel-eslint": "^10.1.0",
    "cypress": "^7.5.0",
    "deploy-aws-s3-cloudfront": "3.4.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-plugin-json-es": "^1.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "~2.1.1",
    "eslint-plugin-vue": "7.10.0",
    "execa": "5.1.0",
    "html-loader": "~1.3.2",
    "node-sass": "~5.0.0",
    "prettier": "2.3.0",
    "sass-loader": "~10.2.0",
    "vue-loader": "~15.9.6",
    "vue-template-compiler": "~2.6.13"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
